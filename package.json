{
  "name": "k2-chrome-extension",
  "version": "0.9.33",
  "description": "A Chrome Extenstion for Kernel Schedule",
  "private": true,
  "scripts": {
    "preinstall": "npx npm-force-resolutions && ./tools/checkRuntimeVersions.sh",
    "watch": "gulp",
    "build": "gulp build",
    "package": "gulp package",
    "zip": "gulp zip",
    "lint": "eslint . --max-warnings=0",
    "web": "webpack --progress --config webpack.dev.js"
  },
  "dependencies": {
    "alt": "^0.17.8",
    "browserify-shim": "^3.8.3",
    "browsernizr": "^1.0.2",
    "jquery": "^2.1.4",
    "moment": "^2.27.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "through2": "2.0.1",
    "underscore": "^1.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "babel-core": "*",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babelify": "^8",
    "browserify": "*",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.7.0",
    "eslint-config-expensify": "^2.0.21",
    "eslint-plugin-react": "^7.28.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gulp": "3.9.1",
    "gulp-copy": "*",
    "gulp-eslint": "*",
    "gulp-ext-replace": "*",
    "gulp-if": "2.0.2",
    "gulp-import-css": "*",
    "gulp-plumber": "*",
    "gulp-react": "*",
    "gulp-rename": "*",
    "gulp-sass": "*",
    "gulp-sourcemaps": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "gulp-zip": "^3.2.0",
    "html-loader": "^3.1.0",
    "mini-css-extract-plugin": "^2.5.2",
    "minimist": "*",
    "node-sass": "^7.0.1",
    "node-underscorify": "*",
    "reactify": "*",
    "run-sequence": "*",
    "sass": "^1.48.0",
    "sass-loader": "^12.4.0",
    "through2": "*",
    "transform-class-properties": "^1.0.0-beta",
    "underscore-template-loader": "^1.1.0",
    "vinyl-buffer": "*",
    "vinyl-source-stream": "*",
    "vinyl-transform": "*",
    "watchify": "*",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "graceful-fs": "4.2.3"
  },
  "browserify-shim": {
    "./public/lib/jquery/dist/jquery.js": {
      "exports": "$"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "node-underscorify",
        {
          "extensions": [
            "html"
          ],
          "requires": [
            {
              "variable": "_",
              "module": "underscore"
            }
          ]
        }
      ]
    ]
  }
}
